{
  "name": "jb-checklists",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "knip": "knip",
    "knip:production": "knip --production",
    "check:all": "npm run type-check && npm run lint && npm run format:check && npm run knip",
    "fix:all": "npm run format && npm run lint:fix",
    "pre-commit": "npm run check:all",
    "ci": "npm run check:all && npm run build",
    "analyze": "npx @next/bundle-analyzer",
    "db:types": "supabase gen types typescript --project-id YOUR_PROJECT_ID > src/types/database.types.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/query-persist-client-core": "^5.81.5",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "clsx": "^2.1.1",
    "next": "14.2.30",
    "next-pwa": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.5.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.4",
    "@types/node": "^20.19.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "eslint": "^8",
    "eslint-config-next": "14.2.30",
    "eslint-config-prettier": "^10.1.5",
    "knip": "^5.61.3",
    "postcss": "^8",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
